   "source": [
    "N = int(input())\n",
    "count = 0\n",
    "\n",
    "while True:\n",
    "    if (N % 5) == 0:\n",
    "        count = count + (N//5)\n",
    "        print(count)\n",
    "        break\n",
    "    \n",
    "    N = N - 3\n",
    "    count += 1\n",
    "    \n",
    "    if N < 0:\n",
    "        print(\"-1\")\n",
    "        break"
   ],
